  <script>
    function validateForm() {
let a = document.forms["myForm"]["Name"].value;
let b = document.forms["myForm"]["Email"].value;
let c = document.forms["myForm"]["Contact number"].value;
let d = document.forms["myForm"]["Subject"].value;
let e = document.forms["myForm"]["Comments"].value;

if (a == "") {
alert("Name must be filled out");
return false;
}
if (b == "") {
alert("Email must be filled out");
return false;
}
if (c == "") {
alert("Contact number must be filled out");
return false;
}
if (d == "") {
alert("Subject must be filled out");
return false;
}
if (e == "") {
alert("Message must be filled out");
return false;
}

}

window.onbeforeunload = () => {
  for(const form of document.getElementsByTagName('form')) {
    form.reset();
  }
}
</script>


